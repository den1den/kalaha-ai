                    <center>
                        <Circle radius="20.0" BorderPane.alignment="CENTER"/>
                    </center>
                </BorderPane>
            </children>
            <columnConstraints>
                <ColumnConstraints prefWidth="100"/>
                <ColumnConstraints prefWidth="100"/>
